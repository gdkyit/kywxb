.p-body{
    padding: 8rpx 16rpx 16rpx 16rpx
}
.p-footer {
  color: #999999;
  font-size: 24rpx;
  text-align: center;
  padding: 10rpx;
  position: fixed;
  bottom: 20rpx;
  left: 0;
  right: 0;
}
.u-info {
  padding-top:16rpx;
  border-bottom: 1px solid #e5e5e5
}
.u-info-main {
  background: linear-gradient(to right, #5D8EE8, #89b2f5);
  height: 150rpx;
  border-radius: 12rpx;
  padding: 48rpx 40rpx;
}
.misa-shadow {
    box-shadow: 0 1px 6px rgba(0, 0, 0, 0.1),0 6px 24px rgba(153, 153, 153, 0.5)
}
.u-avatar-wrap{
    background-color: #FAFBFC !important;
    width:160rpx !important;
    height: 160rpx !important;
}
.u-avatar{
    background-color: #ffffff !important;
    width:150rpx !important;
    height: 150rpx !important;
    border:6rpx solid #ECF5FD !important;
}
.um-group{
    margin-top: 40rpx
}
.u-name {
    font-size: 40rpx;
    color:#ffffff;
}
.u-club-level{
    color:#ECF5FD;
    font-size: 20rpx
}
.u-info-personal {
    padding: 24rpx;
}
.u-info-personal p{
    font-size:24rpx;
    color:#666666
}
.box-h3{
    font-size:48rpx;
    color:#333333
}
.box-text {
    font-size:28rpx;
    color:#666666
}
.u-i-box{
    padding:24rpx;
    height: 80rpx;
    text-align: center
}
.p-grid {
    border-top:1px solid #e5e5e5;
    border-bottom: 1px solid #e5e5e5
}
.grid-box{
    text-align: center;
    padding-top:50rpx;
    padding-bottom: 50rpx;
}
.p-grid .grid-box{
    border-right: 1px solid #e5e5e5
}
.p-grid w-col:last-child .grid-box{
    border-right: none
}
.box-icon image{
    width: 100rpx;
    height:100rpx;
}
.p-notice{
    padding: 16rpx 0;
}
.p-assist{
    padding: 16rpx 0;
    text-align: right;
    vertical-align:center
}
