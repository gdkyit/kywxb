

<view class="weui-article__h1 myhead">收到的出题纠错</view> 
<view class="weui-cells__title">收到的纠错总数：{{totalSize}}</view>
<view class="weui-cells weui-cells_after-title">
            <view class="weui-cell" wx:for="{{selectItems}}" wx:key="value">
                <view class="weui-cell__hd" style="position: relative;margin-right: 10px;">
                    <i class="czs-vector-design" style="font-size:28px" ></i>
                </view>
                <view class="weui-cell__bd">
                    <view style="font-size: 16px;">{{item.remark}}</view>
                        <view style="font-size: 13px;color: #888888;">{{item.CONTENT}}</view>
                </view>
                        <view style="display: inline-block;vertical-align:middle; font-size: 13px;color: #888888;">{{item.dz_date}}</view>
            </view>
</view>
<view class="weui-btn-area">
        <button class="weui-btn"  type="defult" bindtap="backTo" >返回</button>
</view>